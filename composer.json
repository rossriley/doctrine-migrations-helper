{
    "name": "rossriley/doctrine-migrations-helper",
    "description": "Setup tool to provide configured Doctrine Migration Commands",
    "license": "MIT",
    "authors": [
        {
            "name": "Ross Riley",
            "email": "riley.ross@gmail.com"
        }
    ],
    "minimum-stability": "dev",

    "require": {
        "doctrine/migrations"   : "~1.0@dev",
        "symfony/console"       : "2.*"
    },

    "autoload": {
        "psr-4": {
            "DoctrineMigrationsHelper\\": "src",
            "DoctrineMigrationsHelper\\Tests\\": "tests"
        }
    },

    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }

}
